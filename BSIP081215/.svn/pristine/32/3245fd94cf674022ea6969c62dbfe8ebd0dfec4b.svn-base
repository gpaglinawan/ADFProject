<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="DetailedExpensesView"
  Version="11.1.1.64.93"
  InheritPersonalization="true"
  FromList="AAP_DETAILED_EXPENSES AapDetailedExpenses"
  BindingStyle="OracleName"
  CustomQuery="false"
  PageIterMode="Full"
  UseGlueCode="false"
  SelectList="AapDetailedExpenses.ALLOWANCE, 
       AapDetailedExpenses.CENTER_ITM_NO, 
       AapDetailedExpenses.COST, 
       AapDetailedExpenses.COST_CENTER, 
       AapDetailedExpenses.EXPENSES_ID, 
       AapDetailedExpenses.FUND, 
       AapDetailedExpenses.FUNDING_OBJ_CODE, 
       AapDetailedExpenses.REQUEST_ID, 
       AapDetailedExpenses.SUB_ALLOWANCE, 
       AapDetailedExpenses.BAP, 
       AapDetailedExpenses.FISCAL_YEAR">
  <DesignTime>
    <Attr Name="_codeGenFlag2" Value="Access|VarAccess"/>
  </DesignTime>
  <ViewAccessor
    Name="RequestsView"
    ViewObjectName="com.fda.aps.model.viewobject.RequestsView"
    RowLevelBinds="true"/>
  <ViewAccessor
    Name="SecurityGroupsMappingROView"
    ViewObjectName="com.fda.aps.model.viewobject.SecurityGroupsMappingROView"
    RowLevelBinds="true"/>
  <ViewAccessor
    Name="BapROView"
    ViewObjectName="com.fda.aps.model.viewobject.ApsReferenceView">
    <ViewCriteriaUsage
      Name="ByRefType"
      FullName="com.fda.aps.model.viewobject.ApsReferenceView.ByRefType"/>
    <ParameterMap>
      <PIMap Variable="bind_RefType">
        <TransientExpression><![CDATA['BAP']]></TransientExpression>
      </PIMap>
    </ParameterMap>
  </ViewAccessor>
  <ViewAccessor
    Name="FiscalYearROView"
    ViewObjectName="com.fda.aps.model.viewobject.FiscalYearRO"
    RowLevelBinds="true"/>
  <ViewAccessor
    Name="FuncdingObjectClassROView"
    ViewObjectName="com.fda.aps.model.viewobject.ApsReferenceView">
    <ViewCriteriaUsage
      Name="ByRefType"
      FullName="com.fda.aps.model.viewobject.ApsReferenceView.ByRefType"/>
    <ParameterMap>
      <PIMap Variable="bind_RefType">
        <TransientExpression><![CDATA['FOC']]></TransientExpression>
      </PIMap>
    </ParameterMap>
  </ViewAccessor>
  <ViewAccessor
    Name="FundROView"
    ViewObjectName="com.fda.aps.model.viewobject.ApsReferenceView">
    <ViewCriteriaUsage
      Name="ByRefType"
      FullName="com.fda.aps.model.viewobject.ApsReferenceView.ByRefType"/>
    <ParameterMap>
      <PIMap Variable="bind_RefType">
        <TransientExpression><![CDATA['FND']]></TransientExpression>
      </PIMap>
    </ParameterMap>
  </ViewAccessor>
  <ListBinding
    Name="LOV_FiscalYear"
    ListVOName="FiscalYearROView"
    ListRangeSize="-1"
    NullValueFlag="start"
    NullValueId="${adfBundle['oracle.javatools.resourcebundle.SystemBundle']['NULL_VALUE_RESID']}"
    MRUCount="0">
    <AttrArray Name="AttrNames">
      <Item Value="FiscalYear"/>
    </AttrArray>
    <AttrArray Name="ListAttrNames">
      <Item Value="Referencecode"/>
    </AttrArray>
    <AttrArray Name="ListDisplayAttrNames">
      <Item Value="Referencecode"/>
    </AttrArray>
    <DisplayCriteria/>
  </ListBinding>
  <ListBinding
    Name="LOV_FundingObjCode"
    ListVOName="FuncdingObjectClassROView"
    ListRangeSize="-1"
    NullValueFlag="none"
    NullValueId="LOV_FundingObjCode_LOVUIHints_NullValueId"
    MRUCount="0">
    <AttrArray Name="AttrNames">
      <Item Value="FundingObjCode"/>
    </AttrArray>
    <AttrArray Name="ListAttrNames">
      <Item Value="ReferenceCode"/>
    </AttrArray>
    <AttrArray Name="ListDisplayAttrNames">
      <Item Value="ReferenceDescription"/>
    </AttrArray>
    <DisplayCriteria/>
  </ListBinding>
  <EntityUsage
    Name="AapDetailedExpenses"
    Entity="com.fda.aps.model.entity.AapDetailedExpenses"/>
  <ViewAttribute
    Name="Allowance"
    PrecisionRule="true"
    EntityAttrName="Allowance"
    EntityUsage="AapDetailedExpenses"
    AliasName="ALLOWANCE"/>
  <ViewAttribute
    Name="CenterItmNo"
    PrecisionRule="true"
    EntityAttrName="CenterItmNo"
    EntityUsage="AapDetailedExpenses"
    AliasName="CENTER_ITM_NO">
    <Properties>
      <SchemaBasedProperties>
        <LABEL
          ResId="com.fda.aps.model.viewobject.DetailedExpensesView.CenterItmNo_LABEL"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="Cost"
    PrecisionRule="true"
    EntityAttrName="Cost"
    EntityUsage="AapDetailedExpenses"
    AliasName="COST">
    <Properties>
      <SchemaBasedProperties>
        <LABEL
          ResId="com.fda.aps.model.viewobject.DetailedExpensesView.Cost_LABEL"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="CostCenter"
    PrecisionRule="true"
    EntityAttrName="CostCenter"
    EntityUsage="AapDetailedExpenses"
    AliasName="COST_CENTER">
    <Properties>
      <SchemaBasedProperties>
        <LABEL
          ResId="com.fda.aps.model.viewobject.DetailedExpensesView.CostCenter_LABEL"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="ExpensesId"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="ExpensesId"
    EntityUsage="AapDetailedExpenses"
    AliasName="EXPENSES_ID"/>
  <ViewAttribute
    Name="Fund"
    PrecisionRule="true"
    EntityAttrName="Fund"
    EntityUsage="AapDetailedExpenses"
    AliasName="FUND"/>
  <ViewAttribute
    Name="FundingObjCode"
    PrecisionRule="true"
    EntityAttrName="FundingObjCode"
    EntityUsage="AapDetailedExpenses"
    AliasName="FUNDING_OBJ_CODE"
    LOVName="LOV_FundingObjCode">
    <Properties>
      <SchemaBasedProperties>
        <LABEL
          ResId="com.fda.aps.model.viewobject.DetailedExpensesView.FundingObjCode_LABEL"/>
        <CONTROLTYPE
          Value="choice"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="RequestId"
    PrecisionRule="true"
    EntityAttrName="RequestId"
    EntityUsage="AapDetailedExpenses"
    AliasName="REQUEST_ID"/>
  <ViewAttribute
    Name="SubAllowance"
    PrecisionRule="true"
    EntityAttrName="SubAllowance"
    EntityUsage="AapDetailedExpenses"
    AliasName="SUB_ALLOWANCE">
    <Properties>
      <SchemaBasedProperties>
        <LABEL
          ResId="com.fda.aps.model.viewobject.DetailedExpensesView.SubAllowance_LABEL"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="TotalCost"
    IsUpdateable="false"
    IsSelected="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="BIGINT"
    AliasName="VIEW_ATTR"
    SQLType="NUMERIC">
    <TransientExpression><![CDATA[object.getRowSet().sum('Cost')]]></TransientExpression>
  </ViewAttribute>
  <ViewAttribute
    Name="TotAmtRem"
    IsUpdateable="false"
    IsSelected="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="BIGINT"
    AliasName="VIEW_ATTR"
    SQLType="NUMERIC">
    <RecalcCondition><![CDATA[true]]></RecalcCondition>
    <TransientExpression><![CDATA[RequestsView.findByKey(key(RequestId),1)[0].TotalDlarsForCurtFy - (TotalCost == null? 0:TotalCost)]]></TransientExpression>
  </ViewAttribute>
  <ViewAttribute
    Name="Bap"
    PrecisionRule="true"
    EntityAttrName="Bap"
    EntityUsage="AapDetailedExpenses"
    AliasName="BAP">
    <Properties>
      <SchemaBasedProperties>
        <LABEL
          ResId="com.fda.aps.model.viewobject.DetailedExpensesView.Bap_LABEL"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="FiscalYear"
    PrecisionRule="true"
    EntityAttrName="FiscalYear"
    EntityUsage="AapDetailedExpenses"
    AliasName="FISCAL_YEAR"
    LOVName="LOV_FiscalYear">
    <Properties>
      <SchemaBasedProperties>
        <CONTROLTYPE
          Value="choice"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="com.fda.aps.model.APSModelBundle"/>
  </ResourceBundle>
</ViewObject>
