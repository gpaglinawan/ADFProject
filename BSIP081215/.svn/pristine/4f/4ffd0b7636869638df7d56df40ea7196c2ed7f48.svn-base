<?xml version="1.0" encoding="windows-1252"?>
<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee">
  <application>
    <default-render-kit-id>oracle.adf.rich</default-render-kit-id>
    <resource-bundle>
      <base-name>com.fda.aps.view.AAPResourceBundle</base-name>
      <var>aap</var>
    </resource-bundle>
  </application>
  <managed-bean>
    <managed-bean-name>aapBean</managed-bean-name>
    <managed-bean-class>com.fda.aps.view.managedbean.AAPManagedBean</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
</faces-config>